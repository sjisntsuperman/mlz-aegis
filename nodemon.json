{
  "watch": ["src"],
  "ext": "js,ts,json",
  "verbose": false,
  "execMap": {
    "js": "node --inspect",
    "ts": "ts-node -T"
  },
  "events": {
    "start": "rimraf dist && tsc && node scripts/notifier Compile TS End",
    "restart": "node scripts/notifier Restart Server"
  },
  "env": {
    "NODE_ENV": "development"
  }
}
